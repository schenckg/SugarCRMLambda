<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <AWSProjectType>Lambda</AWSProjectType>
    <PackageId>SugarCRMLambda_Grant_Schenck</PackageId>
    <Authors>Grant Schenck</Authors>
    <Description>Amazon Connect Lambda Function to lookup calling phone number and return matching Sugar CRM Contact name if any matching records.</Description>
    <PackageTags>Amazon AWS AmazonAWS Connect Lambda Sugar CRM SugarCRM Contact Lookup DataDip</PackageTags>
    <PackageReleaseNotes />
    <Version>1.0.1</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Amazon.Lambda.Core" Version="1.1.0" />
    <PackageReference Include="Amazon.Lambda.Serialization.Json" Version="1.5.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SugarCRMLibrary\SugarCRMLibrary.csproj" />
  </ItemGroup>
</Project>